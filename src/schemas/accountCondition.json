{
  "description": "Account condition json schema for fastify validation",
  "type": "object",
  "properties": {
    "evtTp": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "type": "string",
        "enum": [
          "pacs.008.001.10",
          "pacs.002.001.12",
          "pain.013.001.09",
          "pain.001.001.11"
        ]
      }
    },
    "condTp": {
      "type": "string",
      "minLength": 1,
      "enum": [
        "overridable-block",
        "non-overridable-block",
        "override"
      ]
    },
    "prsptv": {
      "type": "string",
      "minLength": 1,
      "enum": [
        "creditor",
        "debtor",
        "both"
      ]
    },
    "incptnDtTm": {
      "type": "string",
      "minLength": 1
    },
    "xprtnDtTm": {
      "type": "string",
      "minLength": 1
    },
    "condRsn": {
      "type": "string",
      "minLength": 1
    },
    "acct": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "minLength": 1
        },
        "schmeNm": {
          "type": "object",
          "properties": {
            "prtry": {
              "type": "string",
              "minLength": 1
            }
          },
          "required": [
            "prtry"
          ]
        },
        "agt": {
          "type": "object",
          "properties": {
            "finInstnId": {
              "type": "object",
              "properties": {
                "clrSysMmbId": {
                  "type": "object",
                  "properties": {
                    "mmbId": {
                      "type": "string",
                      "minLength": 1
                    }
                  },
                  "required": [
                    "mmbId"
                  ]
                }
              },
              "required": [
                "clrSysMmbId"
              ]
            }
          },
          "required": [
            "finInstnId"
          ]
        }
      },
      "required": [
        "id",
        "schmeNm",
        "agt"
      ]
    },
    "forceCret": {
      "type": "boolean"
    },
    "usr": {
      "type": "string",
      "minLength": 1
    }
  },
  "required": [
    "evtTp",
    "condTp",
    "prsptv",
    "condRsn",
    "acct",
    "forceCret",
    "usr"
  ]
}
